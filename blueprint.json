{
	"package": "package.json",
	"blueprint": "SCHEMA.md",
	"input": "SCHEMA.md",
	"output": "README.md",
	"logo": {
		"src": "./assets/*",
		"width": 150,
		"height": 150
	},
	"headingPrefix": {
		"1":"",
		"2":""
	},
	"placeholder": [
		"(",
		")"
